    "pm2:stop": "pm2 stop south-delhi-real-estate",
    "pm2:restart": "pm2 restart south-delhi-real-estate",
    "pm2:reload": "pm2 reload south-delhi-real-estate",
    "pm2:delete": "pm2 delete south-delhi-real-estate",
